@model IEnumerable<CategoryDto>
@{
    ViewData["Title"] = "Index";
}

<h1>Kategoriler</h1>


<table>
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>