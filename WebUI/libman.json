{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.4.0",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.min.js"
      ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files" :  ["jquery.min.js"] 
    }
  ,
    {
      "library": "bootstrap-sweetalert@1.0.1",
      "destination": "wwwroot/lib/swal/bootstrap-sweetalert/",
      "files": [
        "sweetalert.min.js",
        "sweetalert.min.css"
      ]
    },
    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jqueryValidate",
